{
  "name": "code-eol",
  "displayName": "code-eol(blame annotation friendly)",
  "description": "Display newline characters in your code",
  "version": "1.0.0",
  "publisher": "r-okm",
  "repository": {
    "url": "https://github.com/r-okm/code-eol"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.15.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "scripts": {
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^1.1.5",
    "typescript": "^2.5.3",
    "@types/node": "7.0.4"
  },
  "dependencies": {
    "vsce": "^1.36.2"
  },
  "contributes": {
    "configuration": [
      {
        "type": "object",
        "title": "code-eol Configuration",
        "properties": {
          "code-eol.color": {
            "type": [
              "string",
              "null"
            ],
            "default": null
          }
        }
      }
    ]
  }
}
